@model RecentActivity
@{
    Html.SetBodyClass("layout02 listadoMyGnoss listadoSuscripciones ");
    
    UserProfileModel Perfil = Html.GetPerfil();
}

<div id="col01">
    <div class="mgHerramientas">
        @Html.PartialView("../Shared/ControlesMVC/_Novedades", "Suscriptions")
    </div>
</div>
<div id="col02">
    <h2>@Html.GetText("MENU", "SUSCRIPCIONES")</h2>
    <div>
        <div id="otherTools" class="otherTools">
            <div id="gestionarSuscripciones" class="gestionarSuscripciones">
                <p><a href="@Perfil.Url@Html.GetText("URLSEM", "ADMINISTRARSUSCRIPCIONES")">@Html.GetText("SUSCRIPCIONES", "ADMINISTRARSUSCRIPCIONES")</a></p>
            </div>
        </div>
        <p class="tituloComponente"></p>
        <div class="resource-list social-events-list widthTabs">
            @if (Model.RecentActivityItems.Count > 0)
            {
                @Html.PartialView("ControlesMVC/_ActividadReciente", Model)
            }
        </div>
    </div>
</div>
