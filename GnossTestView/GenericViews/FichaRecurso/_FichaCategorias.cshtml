@model ResourceModel

@if (Model.Categories != null && Model.Categories.Count>0)
{
	<div class="group categorias" xmlns:skos="http://www.w3.org/2004/02/skos/core#">
		<p>
			@Html.GetText("COMMON", "CATEGORIAS"): 
		</p>
		<ul id="listCat">
             @foreach (CategoryModel categoria in Model.Categories)
            {
            <li rel="sioc:topic">
                <a typeof="skos:Concept" href="@Model.UrlSearch/@Html.GetText("URLSEM","CATEGORIA")/@Html.EliminarCaracteresUrlSem(Html.ObtenerTextoDeIdioma(categoria.LanguageName))/@categoria.Key/"><span property="skos:prefLabel" >@Html.ObtenerTextoDeIdioma(categoria.LanguageName)</span></a>@if (!categoria.Key.Equals(Model.Categories.Last().Key)){@(",")}
            </li>
            }
		</ul>
	</div>
}
