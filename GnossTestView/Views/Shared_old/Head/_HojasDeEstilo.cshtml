@{ 
    CommunityModel Comunidad = Html.GetComunidad();
}

@if (Html.GetJSYCSSunificado().HasValue && Html.GetJSYCSSunificado().Value)
{
    <link media="screen" type="text/css" rel="stylesheet" href="@Html.GetBaseUrlStatic()/cssNuevo/unificado.css?v=@Html.GetVersion()" />
}
else
{    
    <link media="screen" type="text/css" rel="stylesheet" href="@Html.GetBaseUrlStatic()/cssNuevo/reset.css?v=@Html.GetVersion()" />
    <link media="screen" type="text/css" rel="stylesheet" href="@Html.GetBaseUrlStatic()/cssNuevo/header.css?v=@Html.GetVersion()" />
    <link media="screen" type="text/css" rel="stylesheet" href="@Html.GetBaseUrlStatic()/cssNuevo/theme.css?v=@Html.GetVersion()" />
    <link media="screen" type="text/css" rel="stylesheet" href="@Html.GetBaseUrlStatic()/cssNuevo/layout.css?v=@Html.GetVersion()" />
    <link media="screen" type="text/css" rel="stylesheet" href="@Html.GetBaseUrlStatic()/cssNuevo/gnoss.css?v=@Html.GetVersion()" />
    <link media="screen" type="text/css" rel="stylesheet" href="@Html.GetBaseUrlStatic()/cssNuevo/print.css?v=@Html.GetVersion()" />
    <link media="screen" type="text/css" rel="stylesheet" href="@Html.GetBaseUrlStatic()/cssNuevo/jquery.autocomplete.css?v=@Html.GetVersion()" />
    
    <link media="screen" type="text/css" rel="stylesheet" href="@Html.GetBaseUrlStatic()/cssNuevo/riam.css?v=@Html.GetVersion()" />
}

@if (Html.GetListaCSS() != null)
{
    foreach (string css in Html.GetListaCSS())
    {
    <link media="screen" type="text/css" rel="stylesheet" href="@css" />
    }
}

@if (Comunidad != null && Comunidad.Key == ProyectoAD.MetaProyecto)
{ 
    <link media="screen" type="text/css" rel="stylesheet" href="@Html.GetBaseUrlStatic()/cssNuevo/mygnoss.css?v=@Html.GetVersion()" /> 
}

<link media="screen" type="text/css" rel="stylesheet" href="@Html.GetBaseUrlStatic()/lang/es/idioma.css?v=@Html.GetVersion()" />
<link media="screen" type="text/css" rel="stylesheet" href="//code.jquery.com/ui/1.10.3/themes/smoothness/jquery-ui.css" />
<link media="screen" type="text/css" rel="stylesheet" href="//fonts.googleapis.com/css?family=Nunito|Open Sans:300,400,600,700&amp;subset=latin" />

@if (Comunidad != null && Comunidad.VersionCSS.HasValue)
{       
    <link media="screen" type="text/css" rel="stylesheet" href="@Html.GetBaseUrlContent()/imagenes/proyectos/personalizacion/@Comunidad.Key.ToString()@Html.GetStyleVersion()/community.css?v=@Comunidad.VersionCSS" />
}
@if (Comunidad != null && Comunidad.VersionCSSAdmin.HasValue)
{       
    @*<link media="screen" type="text/css" rel="stylesheet" href="@Html.GetBaseUrlContent()/imagenes/proyectos/personalizacion/@Comunidad.Key.ToString()/admin.css?v=@Comunidad.VersionCSSAdmin" />*@
}