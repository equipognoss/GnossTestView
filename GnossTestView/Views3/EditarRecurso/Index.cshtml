@model EditResourceModel
@{
    CommunityModel Comunidad = Html.GetComunidad();
    
    Html.SetBodyClass("layout03 editarRecurso");
    Html.GetListaJS().Add(Html.GetBaseUrlStatic() + "/jsUnificar/MVC.EditarRecurso.js?v=" + Html.GetVersion());
}

@section breadcrumb{
    <div class="breadcrumb" id="breadcrumb">
        <div class="box">
            <p>@Html.GetText("LOGIN", "ESTASEN"):</p>
            <ol>
                <li><a href="@Comunidad.Url"><strong>@Html.GetText("COMMON", "HOME")</strong></a></li>
                <li><a href="@Model.UrlPestanya">@Model.TabName</a></li>
                @if (Model.TypePage == Es.Riam.Gnoss.Web.MVC.Models.EditResourceModel.TypePageEditResource.CreateResource)
                {
                    <li>@Html.GetText("PERFILBASE", "SUBIRRECURSOTITULO")</li>
                }
                else
                {
                    <li>@Model.ModifyResourceModel.CurrentSiteMenuPulg</li>
                }
            </ol>
        </div>
    </div>
}

<div id="col02">
    @if (Model.TypePage == Es.Riam.Gnoss.Web.MVC.Models.EditResourceModel.TypePageEditResource.CreateResource)
    {
        @Html.PartialView("_SubirRecurso", Model.CreateResourceModel)
    }
    else
    {
         @Html.PartialView("_ModificarRecurso", Model.ModifyResourceModel)
    }
</div>