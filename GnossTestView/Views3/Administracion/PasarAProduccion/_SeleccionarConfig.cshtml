<div id="panFormulario">
    <form autocomplete="off">
        <p>
            Selecciona las configuraciones que deseas pasar a producción
        </p>
        <ul class="table">
            <li><input type="checkbox" id="ALL" />Todas las configuraciones</li>
            @if (!ViewBag.EsPaginaEcosistema)
            {
                <li><input type="checkbox" id="Ontologias" />Ontologias</li>
                <li><input type="checkbox" id="ObjetosConocimiento" />Objetos de conocimiento</li>
                <li><input type="checkbox" id="Facetas" />Facetas</li>
                <li><input type="checkbox" id="ComponentesCMS" />Componentes del CMS</li>
                <li><input type="checkbox" id="Gadgets" />Gadgets</li>
                <li><input type="checkbox" id="Pestanyas" />Pestañas</li>
                <li><input type="checkbox" id="PaginasCMS" />Paginas del CMS</li>
                <li><input type="checkbox" id="Utilidades" />Utilidades</li>
                <li><input type="checkbox" id="OpcionesAvanzadas" />Opciones avanzadas</li>
                <li><input type="checkbox" id="ObjetosMultimedia" />Objetos multimedia</li>
                <li><input type="checkbox" id="Estilos" />Estilos</li>
                <li><input type="checkbox" id="Vistas" />Vistas</li>
            }
            else
            {
                <li><input type="checkbox" id="EstilosEcosistema" />Estilos del ecosistema</li>
                <li><input type="checkbox" id="VistasEcosistema" />Vistas del ecosistema</li>
            }
        </ul>
        @*<p>
            <span class="tipo" title="Login o email del usuario Administrador de la comunidad en PRE">Login o email del usuario Administrador de la comunidad en PRE</span>
            <input type="text" id="userName" name="userName" style="width: 460px; margin-left: 14px;" />
        </p>*@
        <input type="button" id="ObtenerConfiguracion" class="guardarTodo" value="Pasar a producción" />
    </form>
</div>
<div id="panHistorial">
    <p><a class="viewHistory">Ver historial</a></p>
    <div class="versionList"></div>
</div>